{
  "name": "@lvce-editor/renderer-worker",
  "version": "0.0.0-dev",
  "description": "",
  "main": "src/rendererWorkerMain.ts",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --max-old-space-size=4096 --unhandled-rejections=warn --experimental-vm-modules ../../node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "build-min": "microbundle --entry src/rendererWorkerMain.js --output distmin --format modern --no-compress",
    "build-min2": "microbundle --entry src/parts/Editor/Editor.js --output distmin --format modern --no-compress",
    "build-min3": "microbundle --entry src/rendererWorkerMain.js --output distmin --format modern",
    "build-min4": "microbundle --entry src/parts/Editor/Editor.js --output distmin --format modern",
    "build-min5": "microbundle --entry src/parts/ViewletEditorText/ViewletEditorText.ipc.js --output distmin --format modern",
    "lint": "npx xo .",
    "type-check": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "xo": {
    "rules": {
      "unicorn/filename-case": "off",
      "indent": "off",
      "semi": "off",
      "no-unused-vars": "off",
      "unicorn/numeric-separators-style": "off",
      "no-extra-semi": "off",
      "arrow-body-style": "off",
      "padded-blocks": "off",
      "capitalized-comments": "off",
      "padding-line-between-statements": "off",
      "arrow-parens": "off",
      "no-warning-comments": "off",
      "array-bracket-spacing": "off",
      "comma-spacing": "off",
      "unicorn/no-array-callback-reference": "off",
      "comma-dangle": "off",
      "operator-linebreak": "off",
      "no-case-declarations": "off",
      "no-undef": "off",
      "object-curly-spacing": "off",
      "object-shorthand": "off",
      "complexity": "off",
      "no-labels": "off",
      "no-multi-assign": "off",
      "max-params": "off",
      "no-bitwise": "off",
      "unicorn/prefer-math-trunc": "off",
      "no-await-in-loop": "off",
      "unicorn/prefer-add-event-listener": "off",
      "no-unused-expressions": "off",
      "unicorn/better-regex": "off",
      "unicorn/no-array-push-push": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/switch-case-braces": "off",
      "unicorn/no-for-loop": "off"
    },
    "ignores": [
      "distmin"
    ]
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^/static/(.*)$": "<rootDir>/../../static/$1",
      "^/js/(.*)$": "<rootDir>/../../static/js/$1",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "dependencies": {
    "@lvce-editor/about-view": "^4.17.0",
    "@lvce-editor/diff-worker": "^1.5.0",
    "@lvce-editor/editor-worker": "^5.17.0",
    "@lvce-editor/explorer-view": "^1.27.0",
    "@lvce-editor/extension-detail-view": "^3.17.0",
    "@lvce-editor/extension-host-sub-worker": "^3.14.0",
    "@lvce-editor/extension-host-worker": "^4.14.0",
    "@lvce-editor/extension-search-view": "^2.12.0",
    "@lvce-editor/file-search-worker": "^4.4.0",
    "@lvce-editor/iframe-worker": "^5.31.0",
    "@lvce-editor/keybindings-view": "^3.17.0",
    "@lvce-editor/markdown-worker": "^1.3.0",
    "@lvce-editor/renderer-process": "^11.2.0",
    "@lvce-editor/syntax-highlighting-worker": "^2.0.1",
    "@lvce-editor/terminal-worker": "^1.2.0",
    "@lvce-editor/test-worker": "^4.5.0",
    "@lvce-editor/text-search-worker": "^3.51.0",
    "@lvce-editor/iframe-inspector": "^1.20.0",
    "@lvce-editor/title-bar-worker": "^1.9.0"
  }
}
