{
  "name": "@lvce-editor/extension-host-worker-tests",
  "version": "0.0.0-dev",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "serve": "node src/_all.js --ci --serve",
    "e2e": "node src/_all.js",
    "e2e:ci": "node src/_all.js --ci",
    "e2e:headless": "node src/_all.js --headless",
    "#e2e:headless:ci": "node src/_all.js --headless --ci",
    "e2e:headless:ci": "echo ok",
    "type-check": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@lvce-editor/test-with-playwright": "^12.1.0",
    "@playwright/test": "^1.57.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "express": "^5.2.1"
  },
  "xo": {
    "rules": {
      "unicorn/filename-case": "off",
      "indent": "off",
      "semi": "off",
      "no-unused-vars": "off",
      "unicorn/numeric-separators-style": "off",
      "no-extra-semi": "off",
      "arrow-body-style": "off",
      "padded-blocks": "off",
      "capitalized-comments": "off",
      "padding-line-between-statements": "off",
      "arrow-parens": "off",
      "no-warning-comments": "off",
      "array-bracket-spacing": "off",
      "comma-spacing": "off",
      "unicorn/no-array-callback-reference": "off",
      "comma-dangle": "off",
      "operator-linebreak": "off",
      "no-case-declarations": "off",
      "no-undef": "off",
      "object-curly-spacing": "off",
      "object-shorthand": "off",
      "complexity": "off",
      "no-labels": "off",
      "no-multi-assign": "off",
      "max-params": "off",
      "no-bitwise": "off",
      "unicorn/prefer-math-trunc": "off",
      "no-await-in-loop": "off",
      "unicorn/prefer-add-event-listener": "off",
      "no-unused-expressions": "off",
      "unicorn/better-regex": "off",
      "unicorn/no-array-push-push": "off",
      "unicorn/prevent-abbreviations": "off"
    },
    "ignores": [
      "distmin"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
